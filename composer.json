{
    "name": "sicoob/sdk-cobranca-pix",
    "description": "SDK PHP para integração com APIs do Sicoob - Cobrança Bancária e PIX",
    "type": "library",
    "license": "MIT",
    "keywords": ["sicoob", "api", "cobranca", "pix", "boleto", "php"],
    "authors": [
        {
            "name": "Sicoob SDK Team",
            "email": "dev@sicoob.com.br"
        }
    ],
            "require": {
            "php": ">=5.6.0",
            "ext-curl": "*",
            "ext-json": "*",
            "ext-openssl": "*"
        },
        "require-dev": {
            "phpunit/phpunit": "^9.0",
            "squizlabs/php_codesniffer": "^3.0"
        },
    "autoload": {
        "psr-4": {
            "Sicoob\\SDK\\": "src/SicoobSDK/"
        }
    },
            "autoload-dev": {
            "psr-4": {
                "Sicoob\\SDK\\Tests\\": "tests/SicoobSDK/"
            }
        },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "cs": "phpcs src/ tests/",
        "cs-fix": "phpcbf src/ tests/"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 